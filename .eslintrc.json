{    "root": true,
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["airbnb", "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console": "off",
        "no-undef": "off",
        "no-multi-assign": "off"
    },
    "settings": {
			"react": {
				"version": "latest"
			}
		}

}
